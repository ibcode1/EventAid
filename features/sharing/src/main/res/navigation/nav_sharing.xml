<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_sharing"
    app:startDestination="@id/sharingFragment">

    <fragment
        android:id="@+id/sharingFragment"
        android:name="com.ib.eventaid.sharing.presentation.SharingFragment"
        android:label="@string/sharing_module_name"
        tools:layout="@layout/fragment_sharing">
        <deepLink app:uri="eventaid://sharing/{id}"/>

        <argument
            android:name="id"
            app:argType="integer"/>

    </fragment>

</navigation>